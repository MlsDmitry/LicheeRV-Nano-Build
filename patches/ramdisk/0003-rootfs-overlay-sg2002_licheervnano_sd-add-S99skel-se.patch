From d90488e6679d3fe714bc0f0ccad405cd886f51ba Mon Sep 17 00:00:00 2001
From: Lu Hui <luhui@sipeed.com>
Date: Mon, 19 Feb 2024 19:23:53 +0800
Subject: [PATCH 3/4] rootfs: overlay: sg2002_licheervnano_sd: add S99skel &
 sensor_cfg.ini for default camera config file

---
 .../sg2002_licheervnano_sd/etc/init.d/S99skel |  7 ++++
 .../sg2002_licheervnano_sd/etc/sensor_cfg.ini | 34 +++++++++++++++++++
 2 files changed, 41 insertions(+)
 create mode 100755 rootfs/overlay/sg2002_licheervnano_sd/etc/init.d/S99skel
 create mode 100644 rootfs/overlay/sg2002_licheervnano_sd/etc/sensor_cfg.ini

diff --git a/rootfs/overlay/sg2002_licheervnano_sd/etc/init.d/S99skel b/rootfs/overlay/sg2002_licheervnano_sd/etc/init.d/S99skel
new file mode 100755
index 0000000..ed2b606
--- /dev/null
+++ b/rootfs/overlay/sg2002_licheervnano_sd/etc/init.d/S99skel
@@ -0,0 +1,7 @@
+#!/bin/sh
+
+# copy default camera config file
+if [ ! -e /mnt/data/sensor_cfg.ini ]
+then
+	cp /etc/sensor_cfg.ini /mnt/data
+fi
diff --git a/rootfs/overlay/sg2002_licheervnano_sd/etc/sensor_cfg.ini b/rootfs/overlay/sg2002_licheervnano_sd/etc/sensor_cfg.ini
new file mode 100644
index 0000000..c1f4c08
--- /dev/null
+++ b/rootfs/overlay/sg2002_licheervnano_sd/etc/sensor_cfg.ini
@@ -0,0 +1,34 @@
+[source]
+
+;type = SOURCE_USER_FE
+
+dev_num = 1
+
+; section for sensor
+
+[sensor]
+
+; sensor name
+name = GCORE_GC4653_MIPI_4M_30FPS_10BIT 
+
+bus_id = 4
+
+sns_i2c_addr = 29
+
+mipi_dev = 0
+
+lane_id = 2, 1, 0, -1, -1
+
+pn_swap = 0, 0, 0, 0, 0
+
+mclk_en = 1
+
+mclk = 0
+
+;port = 0
+
+;pin = 2
+
+;pol = 1
+
+;fps = 30
-- 
2.40.1

